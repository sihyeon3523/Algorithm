    "# 윤년이란 \n",
    "# 연도가 4의 배수이면서, 100의 배수가 아닐 때 또는 400의 배수일 때이다\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "if n % 4 == 0 and (n % 100 != 0 or n % 400 == 0):\n",
    "    print(1)\n",
    "else:\n",
    "    print(0)"
